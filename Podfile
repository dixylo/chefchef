# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'chef chef' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for chef chef

    pod 'FacebookCore'
    pod 'FacebookLogin'
    pod 'FacebookShare'
    pod 'Toucan'
    pod 'Alamofire'
    pod 'AlamofireImage'
    
    
    
    
    platform :ios, '8.0'
    pod 'Firebase','<5'
    pod 'Firebase/Core','<5'
    pod 'Firebase/Auth','<5'
    pod 'Firebase/Database','<5'
    pod 'Firebase/Storage','<5'
    
end
